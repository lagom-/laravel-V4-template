{
  "require": {
		"laravel/framework": "4.0.*",
		"laravel/curl": "dev-master",
		"anahkiasen/former": "dev-agnostic",
		"awareness/aware": "dev-master",
		"jasonlewis/expressive-date": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-update-cmd": "php-cli-5.4 artisan optimize"
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
